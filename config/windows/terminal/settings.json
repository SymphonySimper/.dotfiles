{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "centerOnLaunch": true,
  "launchMode": "maximized",
  "compatibility.enableUnfocusedAcrylic": false,
  "useAcrylicInTabRow": false,
  "copyFormatting": "none",
  "copyOnSelect": true,
  "disableAnimations": true,
  "showTabsInTitlebar": true,
  "tabSwitcherMode": "inOrder",
  "newTabMenu": [{ "type": "remainingProfiles" }],
  "schemes": [
    {
      "name": "Catppuccin Latte",

      "cursorColor": "#DC8A78",
      "selectionBackground": "#ACB0BE",

      "background": "#EFF1F5",
      "foreground": "#4C4F69",

      "black": "#5C5F77",
      "red": "#D20F39",
      "green": "#40A02B",
      "yellow": "#DF8E1D",
      "blue": "#1E66F5",
      "purple": "#EA76CB",
      "cyan": "#179299",
      "white": "#ACB0BE",

      "brightBlack": "#ACB0BE",
      "brightRed": "#D20F39",
      "brightGreen": "#40A02B",
      "brightYellow": "#DF8E1D",
      "brightBlue": "#1E66F5",
      "brightPurple": "#EA76CB",
      "brightCyan": "#179299",
      "brightWhite": "#BCC0CC"
    },
    {
      "name": "Catppuccin Mocha",

      "cursorColor": "#F5E0DC",
      "selectionBackground": "#585B70",

      "background": "#1E1E2E",
      "foreground": "#CDD6F4",

      "black": "#45475A",
      "red": "#F38BA8",
      "green": "#A6E3A1",
      "yellow": "#F9E2AF",
      "blue": "#89B4FA",
      "purple": "#F5C2E7",
      "cyan": "#94E2D5",
      "white": "#BAC2DE",

      "brightBlack": "#585B70",
      "brightRed": "#F38BA8",
      "brightGreen": "#A6E3A1",
      "brightYellow": "#F9E2AF",
      "brightBlue": "#89B4FA",
      "brightPurple": "#F5C2E7",
      "brightCyan": "#94E2D5",
      "brightWhite": "#A6ADC8"
    }
  ],
  "theme": {
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "themes": [
    {
      "name": "Catppuccin Latte",
      "tab": {
        "background": "#EFF1F5FF",
        "showCloseButton": "always",
        "unfocusedBackground": null
      },
      "tabRow": {
        "background": "#E6E9EFFF",
        "unfocusedBackground": "#DCE0E8FF"
      },
      "window": {
        "applicationTheme": "light"
      }
    },
    {
      "name": "Catppuccin Mocha",
      "tab": {
        "background": "#1E1E2EFF",
        "showCloseButton": "always",
        "unfocusedBackground": null
      },
      "tabRow": {
        "background": "#181825FF",
        "unfocusedBackground": "#11111BFF"
      },
      "window": {
        "applicationTheme": "dark"
      }
    }
  ],
  "defaultProfile": "{00000000-0000-0000-0000-000000000001}",
  "profiles": {
    "defaults": {
      "colorScheme": {
        "light": "Catppuccin Latte",
        "dark": "Catppuccin Mocha"
      },
      "cursorShape": "filledBox",
      "font": { "face": "JetBrainsMono Nerd Font" },
      "padding": "2",
      "scrollbarState": "hidden",
      "altGrAliasing": false,
      "historySize": 1000
    },
    "list": [
      {
        "name": "NixOS",
        "guid": "{00000000-0000-0000-0000-000000000001}",
        "commandline": "%SystemRoot%\\System32\\wsl.exe -d NixOS --shell-type login nu",
        "startingDirectory": "~"
      },
      {
        "name": "PowerShell",
        "guid": "{00000000-0000-0000-0000-000000000002}",
        "commandline": "%SystemDrive%\\Program Files\\PowerShell\\7\\pwsh.exe",
        "startingDirectory": "%USERPROFILE%"
      },
      {
        "name": "Windows PowerShell",
        "guid": "{00000000-0000-0000-0000-000000000003}",
        "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
        "startingDirectory": "%USERPROFILE%"
      },
      {
        "name": "Command Prompt",
        "guid": "{00000000-0000-0000-0000-000000000004}",
        "commandline": "%SystemRoot%\\System32\\cmd.exe",
        "startingDirectory": "%USERPROFILE%"
      }
    ]
  },
  "actions": [
    { "id": "User.closeTab", "command": "closeTab" },
    {
      "id": "User.ScrollbackEditor",
      "command": {
        "action": "multipleActions",
        "actions": [
          { "action": "selectAll" },
          { "action": "copy" },
          { "action": "scrollToBottom" },
          {
            "action": "wt",
            "commandline": "--window 0 pwsh -c \"Get-Clipboard | hx\""
          }
        ]
      }
    },
    {
      "id": "User.ScrollbackEditorWSL",
      "command": {
        "action": "multipleActions",
        "actions": [
          { "action": "selectAll" },
          { "action": "copy" },
          { "action": "scrollToBottom" },
          {
            "action": "wt",
            "commandline": "--window 0 --profile NixOS bash -l -c \"win32yank.exe -o | hx\""
          }
        ]
      }
    }
  ],
  "keybindings": [
    { "id": "Terminal.CopyToClipboard", "keys": "ctrl+shift+c" },
    { "id": "Terminal.PasteFromClipboard", "keys": "ctrl+shift+v" },

    { "id": "User.ScrollbackEditorWSL", "keys": "alt+v" },
    { "id": "Terminal.ToggleMarkMode", "keys": "alt+shift+v" },

    { "id": "Terminal.ToggleCommandPalette", "keys": "alt+shift+p" },
    { "id": "Terminal.ToggleFullscreen", "keys": "alt+shift+f" },

    { "id": "Terminal.OpenNewTab", "keys": "alt+t" },
    { "id": "Terminal.OpenNewTabDropdown", "keys": "alt+shift+t" },
    { "id": "User.closeTab", "keys": "alt+shift+c" },
    { "id": "Terminal.SwitchToTab0", "keys": "alt+1" },
    { "id": "Terminal.SwitchToTab1", "keys": "alt+2" },
